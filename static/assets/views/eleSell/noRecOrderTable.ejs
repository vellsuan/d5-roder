<table>
	<thead>
		<tr>
			<th width="20%">国际码</th>
			<th width="20%">名称</th>
			<th width="12%">单价</th>
			<th width="12%">数量</th>
			<th width="12%">折扣</th>
			<th width="12%">金额</th>
			<th width="12%">优惠</th>
		</tr>
	</thead>
	<tbody>
		<% for(var i=0;i<list.length;i++){
			var proNum = 0;
			var price = 0;
			proNum = Number(list[i].num);
			price = Number(list[i].num)*Number(list[i].lessMoney);
		%>
			<tr>
				<td><%=list[i].interCode%></td>
				<td><%=list[i].productName%></td>
				<td><%=Util.formatCurrency(list[i].lessMoney)%></td>
				<td class="pnum"><%=list[i].num%></td>
				<td><%=Util.formatCurrency(list[i].oneMnoey-list[i].lessMoney)%></td>
				<td><%=Util.formatCurrency(price)%></td>
				<td><%=Util.formatCurrency(list[i].disMoney)%></td>
			</tr>
		<%}%>
	</tbody>
</table>