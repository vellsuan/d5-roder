										<div class="original-con-left fleft tableMoneyInfo">
											
										</div>
										<div class="original-con-right fleft">
											<p class="odd-numbers">扫码数量： <em>0</em></p>
											<p class="odd-numbers">数量: <em>0</em></p>
										</div>
									</div>
									<!--按钮-->
									<div class="conbtn">
										<input type="button" orderSN='<%=list[i].orderSn%>' data-orderId=<%=list[i].orderId%> value="接单" class="orders jiedan" />
										<% if(list[i].email && list[i].email =="1"){ %>
											<input type="button" value="已打印" class="print" />
										<%}else{%>
											<input type="button" value="未打印" class="print" />
										<%}%>
										<input type="button" data-orderId=<%=list[i].orderId%> value="拒单" class="single judan"  />
